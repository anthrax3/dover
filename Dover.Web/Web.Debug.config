<?xml version="1.0"?>

<!-- For more information on using web.config transformation visit http://go.microsoft.com/fwlink/?LinkId=125889 -->

<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <!--
    In the example below, the "SetAttributes" transform will change the value of 
    "connectionString" to use "ReleaseSQLServer" only when the "Match" locator 
    finds an atrribute "name" that has a value of "MyDB".
  -->
		<connectionStrings>
			<add name="DoverConnectionString"
        connectionString="Data Source=localhost\SQLEXPRESS;Initial Catalog=uac;Persist Security Info=True;User ID=uac;Password=uac@dmin;MultipleActiveResultSets=True"
        xdt:Transform="SetAttributes" 
        xdt:Locator="Match(name)"/>
      <add name="DoverEntities"
        connectionString="metadata=res://*/Dover.csdl|res://*/Dover.ssdl|res://*/Dover.msl;provider=System.Data.SqlClient;provider connection string=&quot;Data Source=localhost\SQLEXPRESS;Initial Catalog=uac;Persist Security Info=True;User ID=uac;Password=uac@dmin;MultipleActiveResultSets=True&quot;"
        xdt:Transform="SetAttributes"
        xdt:Locator="Match(name)"/>
			<add name="CloudTalkModuleEntities"
        connectionString="metadata=res://*/Areas.CloudTalkModule.Models.CloudTalkModule.csdl|res://*/Areas.CloudTalkModule.Models.CloudTalkModule.ssdl|res://*/Areas.CloudTalkModule.Models.CloudTalkModule.msl;provider=System.Data.SqlClient;provider connection string=&quot;Data Source=localhost\SQLEXPRESS;Initial Catalog=uac;Persist Security Info=True;User ID=uac;Password=uac@dmin;MultipleActiveResultSets=True&quot;"
        xdt:Transform="SetAttributes"
        xdt:Locator="Match(name)"/>
		</connectionStrings>
		<system.web>
    <!--
      In the example below, the "Replace" transform will replace the entire 
      <customErrors> section of your web.config file.
      Note that because there is only one customErrors section under the 
      <system.web> node, there is no need to use the "xdt:Locator" attribute.
    -->

			<customErrors mode="Off" xdt:Transform="Replace">
			</customErrors>
		</system.web>
</configuration>